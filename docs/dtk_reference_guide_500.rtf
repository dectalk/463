\par if (feats & TTS_FEATS_MULTILANG) \{;
\par printf (\'93DECtalk ML installed and running. \\n\'94 ) ;
\par return TRUE ;
\par \}
\par printf (\'93Multi-language DECtalk not found. \\n\'94) ;
