\par printf (\'93DECtalk ML was not found. \\n\'94) ;
\par else if (handle == TTS_NOT_AVAILABLE)
\par printf (\'93English is not currently installed.
\par \\n\'94);
\par else
\par printf (\'93An unknown error has occurred. \\n\'94) ;
\par return FALSE ;}{\f1\fs20\cf1\insrsid10253954 
