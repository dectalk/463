\par }{\f5\fs16\cf1\insrsid1185350 Sub DECtalkTTSmacro()
\par Dim Response, rightNow, diffTime, chan
\par If Tasks.Exists("TTSsrv") = 0 Then
\par StatusBar = "Starting DECtalk Server"
\par System.Cursor = wdCursorWait
\par Response = Shell("TTSSRV.EXE", 0)
\par rightNow = Now()
\par diffTime = 0
\par While diffTime < 0.00003
\par diffTime = Now() - rightNow
\par Wend
\par StatusBar = " "
\par System.Cursor = wdCursorNormal
\par If Response = 0 Then
\par Response = MsgBox("Cannot find TTSSRV.EXE in the path",
\par vbYesNo, "Speak")
\par GoTo FINIS
\par End If
\par End If
\par chan = DDEInitiate(App:="TTSserver", Topic:="Speak")
\par If chan = 0 Then
\par Response = MsgBox("Unable to connect to TTSSRV.EXE", vbYesNo,
\par "Speak")
\par GoTo FINIS
\par Else
\par DDEPoke channel:=chan, Item:="Speak", Data:=Selection.Text
\par DDETerminate channel:=chan
\par End If
\par FINIS:
\par End Sub
