\par UINT uiMsg)
\par \{
\par if (uiMsg == TTS_MSG_STATUS)
\par \{
\par // lParam1 contains error code
\par // lParam2 contains value of MMRESULT
\par \}}{\f1\fs20\cf1\insrsid1185350 
\par }{\f1\fs18\cf1\insrsid1185350 3-11
\par }{\f5\fs18\cf1\insrsid1185350 else if (uiMsg == TTS_MSG_INDEX_MARK)
\par \{
\par // lParam2 contains index mark
\par \}
\par else if (uiMsg == TTS_MSG_BUFFER)
\par \{
\par // lParam2 contains Pointer to buffer
\par \}
\par \}
\par }{\f1\fs21\cf1\insrsid1185350 Window Procedure Example
\par }{\f5\fs18\cf1\insrsid1185350 static UINT uiID_Error_Message = 0;
\par static UNIT uiID_Buffer_Message = 0;
\par static UNIT uiID_Index_Message = 0;
\par // Window Procedure
\par VOID MyWndProc(LONG lParam1, LONG lParam2, DWORD dwCallbackParameter,
\par UINT uiMsg)
\par \{
\par if (uiMsg == uiID_Error_Message)
\par \{
\par // lParam1 contains error code
\par // lParam2 contains value of MMRESULT
\par \}
\par else if (uiMsg == uiID_Index_Message)
\par \{
